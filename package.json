{
  "name": "emenu",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "e2e": "turbo e2e",
    "clean": "turbo clean && rm -rf node_modules",
    "check-types": "turbo check-types"
  },
  "devDependencies": {
    "cypress": "^12.0.0",
    "prettier": "^3.5.0",
    "turbo": "latest",
    "typescript": "latest"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.18.0"
  }
}
